{
    "builds": [{
        "src": "core/wsgi.py",
        "use": "https://bootstrap.pypa.io/pip/3.6/get-pip.py",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        }
    ]
}
